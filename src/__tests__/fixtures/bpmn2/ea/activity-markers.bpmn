<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  id="Defs_ActivityMarkers"
  targetNamespace="http://example.tullverket.se/bpmn">

  <bpmn:process id="Proc_1" isExecutable="false">
    <bpmn:startEvent id="Start_1" name="Start"/>

    <bpmn:userTask id="Task_MI_Par" name="Multi-instance parallel">
      <bpmn:multiInstanceLoopCharacteristics isSequential="false"/>
    </bpmn:userTask>

    <bpmn:serviceTask id="Task_MI_Seq" name="Multi-instance sequential">
      <bpmn:multiInstanceLoopCharacteristics isSequential="true"/>
    </bpmn:serviceTask>

    <bpmn:task id="Task_Loop" name="Standard loop">
      <bpmn:standardLoopCharacteristics/>
    </bpmn:task>

    <bpmn:task id="Task_CompAttr" name="Compensation (attr)" isForCompensation="true"/>

    <bpmn:task id="Task_CompChild" name="Compensation (child)">
      <bpmn:compensateEventDefinition/>
    </bpmn:task>

    <bpmn:endEvent id="End_1" name="End"/>

    <bpmn:sequenceFlow id="Flow_1" sourceRef="Start_1" targetRef="Task_MI_Par"/>
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_MI_Par" targetRef="Task_MI_Seq"/>
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_MI_Seq" targetRef="Task_Loop"/>
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_Loop" targetRef="Task_CompAttr"/>
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_CompAttr" targetRef="Task_CompChild"/>
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_CompChild" targetRef="End_1"/>
  </bpmn:process>
</bpmn:definitions>
